{
  "name": "sqc-nodejs-demo",
  "version": "0.0.0",
  "private": true,
  "main": "src/server/index.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"vite\" \"NODE_ENV=development DEBUG=sqc_app* nodemon ./bin/www\"",
    "build": "vite build",
    "build:image": "vite build && cp -r src/client/assets/images/* dist/assets/",
    "build:server": "tsc --project tsconfig.server.json",
    "build:all": "npm run build && npm run build:server",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "mocha specs/**/*"
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "dependencies": {
    "airtable": "^0.8.1",
    "axios": "^1.12.2",
    "camelcase-keys": "^6.2.2",
    "chai": "^4.5.0",
    "cookie-parser": "~1.4.7",
    "date-fns": "^2.30.0",
    "debug": "~2.6.9",
    "dotenv": "^8.6.0",
    "ejs": "^3.1.10",
    "express": "~4.16.4",
    "history": "^5.3.0",
    "http-errors": "~1.6.3",
    "knex": "^0.20.15",
    "lodash": "^4.17.21",
    "messenger-node": "^0.3.2",
    "mocha": "^8.4.0",
    "morgan": "~1.9.1",
    "pg": "^8.16.3",
    "react": "^16.14.0",
    "react-bootstrap": "^1.6.8",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.3.4",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
