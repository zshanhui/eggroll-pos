    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.3",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  },
